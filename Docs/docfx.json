{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.sln"
          ],
          "src": "../"
        }
      ],
      "dest": "Api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "Api/**.yml",
          "Api/index.md"
        ]
      },
      {
        "files": [
          "Usage/**.md",
          "Usage/**/toc.yml",
          "Development/**.md",
          "Development/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}